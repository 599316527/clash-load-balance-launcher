global
    daemon
    maxconn 256

defaults
    mode ${mode}
    timeout connect 5000ms
    timeout client 50000ms
    timeout server 50000ms

listen socks-in
    bind 127.0.0.1:8882
    balance roundrobin
<!-- for: ${ports} as ${port}, ${index} -->
    server server${index} 127.0.0.1:${port} maxconn 32<!-- /for -->
